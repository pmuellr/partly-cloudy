---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
    <title>Patrick Mueller - partly cloudy</title>
    <link href="http://pmuellr.github.io/partly-cloudy/atom.xml" rel="self"/>
    <link href="http://pmuellr.github.io/partly-cloudy/"/>

    <updated>{{ site.time | date_to_xmlschema }}</updated>
    <id>http://pmuellr.github.io/partly-cloudy/</id>

    <author>
        <name>Patrick Mueller</name>
    </author>

    {% for post in site.posts %}
    <entry>
        <title>{{ post.title }}</title>
        <link href="http://pmuellr.github.io/partly-cloudy{{ post.url }}"/>
        <updated>{{ post.date | date_to_xmlschema }}</updated>
        <id>http://pmuellr.github.io/partly-cloudy{{ post.id }}</id>
        <content type="html">{{ post.content | xml_escape }}</content>
    </entry>

    {% endfor %}
 
</feed>